G/00 - Coding/React App/02 - Clever Programmer/05-amazon-clone/src/App.js",["115","116"],"import React, { useEffect } from 'react';\nimport { BrowserRouter as Router, Switch, Route } from 'react-router-dom';\nimport Header from './Header';\nimport Home from './Home';\nimport Checkout from './Checkout';\nimport Login from './Login';\nimport { useStateValue } from './StateProvider';\nimport { auth } from './firebase';\n\nfunction App() {\n  const [{ basket }, dispatch] = useStateValue();\n\n  // useEffect hook\n  // a Piece of code that runs based upon a given condition\n  useEffect(() => {\n    const unsubscribe = auth.onAuthStateChanged(authUser => {\n      if (authUser) {\n        // the user is logged in\n        dispatch({\n          type: 'SET_USER',\n          user: authUser,\n        });\n      } else {\n        // the user is logged out\n        dispatch({\n          type: 'SET_USER',\n          user: null,\n        });\n      }\n    });\n\n    return () => {\n      // cleanup operations go in here\n      unsubscribe();\n    };\n  }, []);\n\n  return (\n    <Router>\n      <div className='app'>\n        <Switch>\n          <Route path='/checkout'>\n            <Header />\n            <Checkout />\n          </Route>\n          <Route path='/login'>\n            <Login />\n          </Route>\n          <Route path='/'>\n            <Header />\n            <Home />\n          </Route>\n        </Switch>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n",["117","118"],"/Volumes/Data/STUDYING/00 - Coding/React App/02 - Clever Programmer/05-amazon-clone/src/StateProvider.js",[],"/Volumes/Data/STUDYING/00 - Coding/React App/02 - Clever Programmer/05-amazon-clone/src/reducer.js",[],"/Volumes/Data/STUDYING/00 - Coding/React App/02 - Clever Programmer/05-amazon-clone/src/Header.js",[],"/Volumes/Data/STUDYING/00 - Coding/React App/02 - Clever Programmer/05-amazon-clone/src/firebase.js",["119"],"/Volumes/Data/STUDYING/00 - Coding/React App/02 - Clever Programmer/05-amazon-clone/src/Login.js",[],"/Volumes/Data/STUDYING/00 - Coding/React App/02 - Clever Programmer/05-amazon-clone/src/Home.js",[],"/Volumes/Data/STUDYING/00 - Coding/React App/02 - Clever Programmer/05-amazon-clone/src/Checkout.js",["120"],"import React from 'react';\nimport { useStateValue } from './StateProvider';\nimport CheckoutProduct from './CheckoutProduct';\nimport Subtotal from './Subtotal';\n\nfunction Checkout() {\n  const [{ basket }, dispatch] = useStateValue();\n\n  return (\n    <div className='checkout'>\n      <div className='checkout__left'>\n        <img\n          className='checkout__ad'\n          src='https://i.ibb.co/BLfVJ2j/checkout-banner2.jpg'\n          alt='outlet ad'\n        />\n        {basket?.length === 0 ? (\n          <div>\n            <h2>Your shopping basket is empty</h2>\n            <p>\n              You have no items in your basket. To buy one or more items, click\n              \"Add to basket\" next to the item.\n            </p>\n          </div>\n        ) : (\n          <div>\n            <h2>Your shopping basket</h2>\n            {basket?.map(item => (\n              <CheckoutProduct\n                id={item.id}\n                title={item.title}\n                image={item.image}\n                price={item.price}\n                rating={item.rating}\n              />\n            ))}\n          </div>\n        )}\n      </div>\n      {basket.length > 0 && (\n        <div className='checkout__right'>\n          <Subtotal />\n        </div>\n      )}\n    </div>\n  );\n}\n\nexport default Checkout;\n","/Volumes/Data/STUDYING/00 - Coding/React App/02 - Clever Programmer/05-amazon-clone/src/Product.js",["121"],"import React from 'react';\nimport { useStateValue } from './StateProvider';\n\nfunction Product({id, title, price, rating, image}) {\n    const [{ basket }, dispatch] = useStateValue();\n\n    const addToBasket = () => {\n        // Add item to basket\n        dispatch ({\n            type: 'ADD_TO_BASKET',\n            item: {\n                id: id,\n                title: title,\n                image: image,\n                price: price,\n                rating: rating\n            }\n        })\n    };\n    \n    return (\n        <div className='product'>\n            <div className='product__info'>\n                <p>{title}</p>\n                <p className='product__price'>\n                    <small>$</small>\n                    <strong>{price}</strong>\n                </p>\n                <div className='product__rating'>\n                    {\n                        Array(rating)\n                        .fill()\n                        .map((_) => (\n                            <p>⭐</p>\n                        ))\n                    }\n                </div>\n            </div>\n            <img src = {image} alt ={title} />\n            <button onClick={addToBasket}>Add to basket</button>\n        </div>\n    )\n}\n\nexport default Product\n","/Volumes/Data/STUDYING/00 - Coding/React App/02 - Clever Programmer/05-amazon-clone/src/Subtotal.js",["122"],"import React from 'react';\nimport CurrencyFormat from 'react-currency-format';\nimport { useStateValue } from './StateProvider';\nimport { getBasketTotal } from './reducer';\n\nfunction Subtotal() {\n  const [{ basket }, dispatch] = useStateValue();\n\n  return (\n    <div className='subtotal'>\n      {/* Price */}\n      <CurrencyFormat\n        renderText={value => (\n          <>\n            <p>\n              Subtotal ({basket.length} items): <strong>{value}</strong>\n            </p>\n            <small className='subtotal__gift'>\n              <input type='checkbox' />\n              This order contains a gift\n            </small>\n          </>\n        )}\n        decimalScale={2}\n        fixedDecimalScale={true}\n        value={getBasketTotal(basket)}\n        displayType={'text'}\n        thousandSeparator={true}\n        prefix={'$'}\n      />\n      <button>Proceed to checkout</button>\n    </div>\n  );\n}\n\nexport default Subtotal;\n","/Volumes/Data/STUDYING/00 - Coding/React App/02 - Clever Programmer/05-amazon-clone/src/CheckoutProduct.js",["123"],"import React from 'react';\nimport { useStateValue } from './StateProvider';\n\nfunction CheckoutProduct({ id, title, image, price, rating }) {\n  const [{ basket }, dispatch] = useStateValue();\n\n  const RemoveFromBasket = () => {\n    dispatch({\n      type: 'REMOVE_FROM_BASKET',\n      id: id,\n    });\n  };\n\n  return (\n    <div className='checkoutProduct'>\n      <img className='checkoutProduct__image' src={image} alt={title} />\n      <div className='checkoutProduct__info'>\n        <strong className='checkoutProduct__title'>{title}</strong>\n        <p className='checkoutProduct__price'>\n          <small>$</small>\n          <strong>{price}</strong>\n        </p>\n        <div className='checkoutProduct__rating'>\n          {Array(rating)\n            .fill()\n            .map(_ => (\n              <p>⭐</p>\n            ))}\n        </div>\n        <button onClick={RemoveFromBasket}>Remove from basket</button>\n      </div>\n    </div>\n  );\n}\n\nexport default CheckoutProduct;\n","/Volumes/Data/STUDYING/00 - Coding/React App/02 - Clever Programmer/05-amazon-clone/src/serviceWorker.js",[],"/Volumes/Data/STUDYING/00 - Coding/React App/00 - Own Projects/01 - InstaShop/src/index.js",[],["124","125"],"/Volumes/Data/STUDYING/00 - Coding/React App/00 - Own Projects/01 - InstaShop/src/StateProvider.js",[],"/Volumes/Data/STUDYING/00 - Coding/React App/00 - Own Projects/01 - InstaShop/src/App.js",["126","127"],"import React, { useEffect } from 'react';\nimport { BrowserRouter as Router, Switch, Route } from 'react-router-dom';\nimport Header from './Components/Header';\nimport Home from './Components/Home';\nimport Checkout from './Components/Checkout';\nimport Login from './Components/Login';\nimport { useStateValue } from './StateProvider';\nimport { auth } from './firebase';\n